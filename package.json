  },
  "homepage": "https://github.com/huserben/plumbr#readme",
  "dependencies": {
    "@types/jquery": "^3.5.5",
    "@types/q": "^1.5.4",
    "vss-web-extension-sdk": "^5.141.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "tfx-cli": "^0.8.3",
    "tsc": "^1.20150623.0"
  }
