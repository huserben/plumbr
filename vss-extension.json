{
    "manifestVersion": 1,
    "id": "plumbr",
    "publisher": "benjhuser",
    "version": "0.1.23",
    "name": "Plumbr - Your Pipeline Manager",
    "description": "Manage your Azure Pipelines with this extension",
    "public": false,
    "categories": ["Azure Pipelines"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scopes":[
        "vso.build_execute"
    ],
    "contributions": [
        {
            "id": "pumbr",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-build-web.build-release-hub-group"
            ],
            "properties": {
                "name": "Plumbr",
                "uri": "plumbr.html"
            }
        }
    ],
    "files": [
        {
            "path": "plumbr.html",
            "addressable": true
        },
        {
            "path": "plumbr.js",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}